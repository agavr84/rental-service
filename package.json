{
  "name": "rental-service",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:static": "node scripts/build-static.js",
    "build:local": "BASE_PATH=/ SITE_URL=http://localhost:8080 node scripts/build-static.js",
    "db:up": "docker compose up -d",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:seed": "npm run db:generate && npx prisma db seed"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "gray-matter": "^4.0.3",
    "marked": "^14.1.0"
  },
  "devDependencies": {
    "prisma": "5.22.0"
  }
}
